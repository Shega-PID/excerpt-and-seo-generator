{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "excerpt": {
      "type": "text",
      "maxLength": 200,
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "seo.seo"
    },
    "content": {
      "type": "customField",
      "options": {
        "output": "HTML",
        "preset": "standard"
      },
      "required": true,
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
